<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.upstore.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.upstore.myapplication.register_business"></activity>
        <activity
            android:name="com.upstore.myapplication.register"
            android:parentActivityName="com.upstore.myapplication.login" />
        <activity android:name="com.upstore.myapplication.activity_categories" />
        <activity android:name="com.upstore.myapplication.MainActivity" />
        <activity android:name="com.upstore.myapplication.BusinessActivity" />
        <activity
            android:name="com.upstore.myapplication.add_stars"
            android:parentActivityName="com.upstore.myapplication.MainActivity" />
        <activity
            android:name="com.upstore.myapplication.activity_settings"
            android:parentActivityName="com.upstore.myapplication.MainActivity" />
        <activity android:name="com.upstore.myapplication.login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>